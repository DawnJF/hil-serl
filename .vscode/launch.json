    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File with Arguments",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": "${command:pickArgs}"
        },
        {
            "name": "Python Debugger: train_offline.py",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/pytorch_serl/train_offline.py",
            "console": "integratedTerminal",
            "args": [
                "--demo_paths",
                "/Users/majianfei/Downloads/success_demo.pkl",
                "--save_dir",
                "log/test_offline_demo",
                "--max_steps",
                "20",
                "--batch_size",
                "64"
            ]
        }
    ]
}