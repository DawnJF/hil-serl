    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File with Arguments",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": "${command:pickArgs}"
        },
        {
            "name": "learner usb_pickup_insertion",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/examples/train_rlpd.py",
            "console": "integratedTerminal",
            "args": [
                "--exp_name=usb_pickup_insertion",
                // "--checkpoint_path=first_run",
                "--demo_path=/home/facelesswei/code/Jax_Hil_Serl_Dataset/2025-09-09/usb_pickup_insertion_5_11-05-02.pkl",
                "--learner",
                "--debug",
                "--port=5888",
                // "--checkpoint_path=/media/robot/30F73268F87D0FEF/Jax_Hil_Serl_Dataset/2025-09-01/usb_pickup_insertion_5_14-42-28.pkl"
            ],
            "env": {
                "XLA_PYTHON_CLIENT_PREALLOCATE": "false",
                "XLA_PYTHON_CLIENT_MEM_FRACTION": ".3"
            }
        },
        {
            "name": "actor usb_pickup_insertion",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/examples/train_rlpd.py",
            "console": "integratedTerminal",
            "args": [
                "--exp_name=usb_pickup_insertion",
                "--checkpoint_path=../../experiments/usb_pickup_insertion/plug4",
                "--wandb_mode=offline",
                "--wandb_output_dir=../../experiments/usb_pickup_insertion/wandb",
                "--actor",
                "--port=5888",
                "--debug",
            ],
            "env": {
                "XLA_PYTHON_CLIENT_PREALLOCATE": "false",
                "XLA_PYTHON_CLIENT_MEM_FRACTION": ".1"
            }
        },
        {
            "name": "record_demo",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/examples/record_demos.py",
            "console": "integratedTerminal",
            "args": [
                "--exp_name=usb_pickup_insertion",
                "--successes_needed=1"
            ],
            "env": {
                "XLA_PYTHON_CLIENT_PREALLOCATE": "false",
                "XLA_PYTHON_CLIENT_MEM_FRACTION": ".1"
            }
        },
        {
            "name": "usb-config",
            "type": "debugpy",
            "request": "launch",
            // "python": "/usr/bin/sudo",
            "program": "${workspaceFolder}/examples/experiments/usb_pickup_insertion/config.py",
            "console": "integratedTerminal",
            "env": {
                "XLA_PYTHON_CLIENT_PREALLOCATE": "false",
                "XLA_PYTHON_CLIENT_MEM_FRACTION": ".1"
            }
        },
        {
            "name": "Python Debugger with Sudo",
            "type": "debugpy",
            "request": "launch",
            "program": "/usr/bin/sudo",
            "args": [
                "-E", // 保留环境变量
                "/home/robot/miniforge3/envs/roboar4il/hilserl/bin/python",
                "${workspaceFolder}/examples/train_rlpd.py",
                "--exp_name=usb_pickup_insertion", // 您的脚本参数
                "--demo_path=/media/robot/30F73268F87D0FEF/Jax_Hil_Serl_Dataset/2025-09-01/usb_pickup_insertion_5_14-42-28.pkl",
                "--learner",
                "--debug=true",
            ],
            "console": "integratedTerminal",
            "env": {
                "XLA_PYTHON_CLIENT_PREALLOCATE": "false",
                "XLA_PYTHON_CLIENT_MEM_FRACTION": ".1"
            }
        },
        {
            "name": "torch RLPD learner",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/rl/train.py",
            "args": [
                "--learner",
                // "--resume_checkpoint=outputs/torch_rlpd/debug/20250930-1441/checkpoint_400.pt",
                // "--demo_path=/Users/majianfei/Downloads/usb_pickup_insertion_5_11-05-02.pkl",
                "--bc_agent=outputs/bc2rl/20251013_093116/checkpoint-60.pth",
                "--demo_path=/home/facelesswei/code/Jax_Hil_Serl_Dataset/2025-09-09/usb_pickup_insertion_30_11-50-21.pkl"
            ]
        },
        {
            "name": "torch RLPD actor",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/rl/train.py",
            "args": [
                "--actor",
                "--bc_agent=outputs/bc2rl/20251013_093116/checkpoint-60.pth",
            ]
        },
    ]
}